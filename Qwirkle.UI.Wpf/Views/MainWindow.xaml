<Window x:Class="Qwirkle.UI.Wpf.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:viewModels="clr-namespace:Qwirkle.UI.Wpf.ViewModels"
        xmlns:views="clr-namespace:Qwirkle.UI.Wpf.Views"
        Title="Qwirkle" Height="800" Width="1200" WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <DataTemplate DataType="{x:Type viewModels:GameViewModel}">
            <views:GameView></views:GameView>
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <ContentPresenter Content="{Binding CurrentViewModel}"></ContentPresenter>
    </Grid>
</Window>
